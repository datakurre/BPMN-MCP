<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="Process_Order_Processing" name="Order Processing" isExecutable="true" camunda:historyTimeToLive="P180D">
    <bpmn:startEvent id="StartEvent_OrderReceived" name="Order Received">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="orderId" label="Order ID" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="quantity" label="Quantity" type="long">
            <camunda:validation>
              <camunda:constraint name="min" config="1" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="priority" label="Priority" type="enum" defaultValue="medium">
            <camunda:value id="low" name="Low" />
            <camunda:value id="medium" name="Medium" />
            <camunda:value id="high" name="High" />
          </camunda:formField>
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_0rhbzyi</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="UserTask_ReviewOrder" name="Review Order" camunda:formKey="embedded:app:forms/review.html" camunda:assignee="reviewer">
      <bpmn:incoming>Flow_0rhbzyi</bpmn:incoming>
      <bpmn:outgoing>Flow_ReviewOrder_to_OrderValid</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0rhbzyi" sourceRef="StartEvent_OrderReceived" targetRef="UserTask_ReviewOrder" />
    <bpmn:serviceTask id="ServiceTask_CheckInventory" name="Check Inventory" camunda:class="com.example.CheckInventory">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="orderId">${orderId}</camunda:inputParameter>
          <camunda:inputParameter name="qty">${quantity}</camunda:inputParameter>
          <camunda:outputParameter name="inventoryOk">${available}</camunda:outputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_OrderValid_to_CheckInventory</bpmn:incoming>
      <bpmn:outgoing>Flow_0xrrqqq</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="UserTask_ConfirmOrder" name="Confirm Order" camunda:candidateGroups="managers">
      <bpmn:incoming>Flow_0xrrqqq</bpmn:incoming>
      <bpmn:outgoing>Flow_1l7pkgf</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0xrrqqq" sourceRef="ServiceTask_CheckInventory" targetRef="UserTask_ConfirmOrder" />
    <bpmn:endEvent id="EndEvent_OrderCompleted" name="Order Completed">
      <bpmn:incoming>Flow_1l7pkgf</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1l7pkgf" sourceRef="UserTask_ConfirmOrder" targetRef="EndEvent_OrderCompleted" />
    <bpmn:exclusiveGateway id="Gateway_OrderValid" name="Order Valid?" default="Flow_No">
      <bpmn:incoming>Flow_ReviewOrder_to_OrderValid</bpmn:incoming>
      <bpmn:outgoing>Flow_OrderValid_to_CheckInventory</bpmn:outgoing>
      <bpmn:outgoing>Flow_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_ReviewOrder_to_OrderValid" sourceRef="UserTask_ReviewOrder" targetRef="Gateway_OrderValid" />
    <bpmn:sequenceFlow id="Flow_OrderValid_to_CheckInventory" name="Yes" sourceRef="Gateway_OrderValid" targetRef="ServiceTask_CheckInventory">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${orderValid == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="UserTask_HandleRejection" name="Handle Rejection">
      <bpmn:incoming>Flow_No</bpmn:incoming>
      <bpmn:outgoing>Flow_HandleRejection_to_OrderRejected</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_OrderRejected" name="Order Rejected">
      <bpmn:incoming>Flow_HandleRejection_to_OrderRejected</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_No" name="No" sourceRef="Gateway_OrderValid" targetRef="UserTask_HandleRejection" />
    <bpmn:sequenceFlow id="Flow_HandleRejection_to_OrderRejected" sourceRef="UserTask_HandleRejection" targetRef="EndEvent_OrderRejected" />
    <bpmn:boundaryEvent id="Event_wziuoun" cancelActivity="false" attachedToRef="UserTask_ReviewOrder">
      <bpmn:outgoing>Flow_15twkl7</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT24H</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    <bpmn:sendTask id="SendTask_SendReminder" name="Send Reminder">
      <bpmn:incoming>Flow_15twkl7</bpmn:incoming>
      <bpmn:outgoing>Flow_09er024</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:sequenceFlow id="Flow_15twkl7" sourceRef="Event_wziuoun" targetRef="SendTask_SendReminder" />
    <bpmn:endEvent id="EndEvent_ReminderSent" name="Reminder Sent">
      <bpmn:incoming>Flow_09er024</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_09er024" sourceRef="SendTask_SendReminder" targetRef="EndEvent_ReminderSent" />
  </bpmn:process>
